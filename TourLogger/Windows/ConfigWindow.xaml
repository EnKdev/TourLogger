<Window x:Class="TourLogger.Windows.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:TourLogger.Windows"
        mc:Ignorable="d"
        Title="TourLogger - Configuration" Height="154" Width="298" FontFamily="Bahnschrift" ResizeMode="NoResize">
    <Grid>
        <CheckBox Name="chk_Experimental" Click="Chk_Experimental_OnClick" Content="Enable experimental channel" FontSize="18" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button Content="Close" Name="bt_Close" Click="bt_Close_Click" HorizontalAlignment="Left" FontSize="18" Margin="215,92,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
